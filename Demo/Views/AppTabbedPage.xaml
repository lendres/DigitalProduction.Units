<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:UnitsConversionDemo"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom"
            x:Class="UnitsConversionDemo.AppTabbedPage">

    <NavigationPage Title="Convert by Parsing" IconImageSource="{StaticResource TabOne}">
        <x:Arguments>
			<local:ParseView Title="Convert by Parsing" />
        </x:Arguments>
    </NavigationPage>
	<NavigationPage Title="Edit Units" IconImageSource="{StaticResource TabTwo}">
		<x:Arguments>
			<local:UnitsView Title="Edit Units" />
		</x:Arguments>
	</NavigationPage>

</TabbedPage>

			<Multiply>1e-012</Multiply>
		</Unit>
		
		<Unit name="megagram">
			<Symbol>Mg</Symbol>
			<Multiply>1000</Multiply>
		</Unit>
		
		<Unit name="gigagram">
			<Symbol>Gg</Symbol>
			<Multiply>1000000</Multiply>
		</Unit>
		
		<!--<Unit name="tonne">
			<Symbol>t</Symbol>
			<Multiply>1000</Multiply>
		</Unit>-->
		
		<Unit name="ton">
			<Symbol>ton</Symbol>
			<Multiply>907.18474</Multiply>
		</Unit>
		
		<Unit name="stone">
			<Symbol>stone</Symbol>
			<Multiply>6.35029318</Multiply>
		</Unit>
		
